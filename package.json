{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Mein gutes POrtfolio",
  "main": "app.ts",
  "scripts": {
    "start": "npm run build && npm run watch-ts && npm run serve",
    "build": "esbuild src/app.ts --bundle --platform=node --outfile=dist/out.js",
    "serve": "node build/app.js",
    "watch-node": "nodemon -r dotenv/config build/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "mocha -r ts-node/register \"src/tests/**/*.test.ts\"",
    "format": "prettier --write .,",
    "seed": "ts-node ./src/database/seeder.ts -d",
    "dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dmsosa/portfolio-backend.git"
  },
  "keywords": [
    "portfolio",
    "blog"
  ],
  "author": "duvi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmsosa/portfolio-backend/issues"
  },
  "homepage": "https://github.com/dmsosa/portfolio-backend#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "errorhandler": "^1.5.1",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "4.1",
    "mongoose": "^8.10.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "slugify": "^1.6.6",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^5.2.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.3",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^7.4.4",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.17.15",
    "@types/method-override": "0.0.32",
    "@types/mocha": "^10.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.45",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/passport-strategy": "^0.2.38",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "chai": "^5.2.0",
    "eslint": "^9.20.1",
    "mocha": "^11.3.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
